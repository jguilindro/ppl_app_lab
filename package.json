{
  "name": "espol-ppl",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "postinstall": "bower install",
    "dev": "cross-env NODE_ENV=development env-cmd .env nodemon server.js --config nodemon.json",
    "dev:cas": "cross-env NODE_ENV=development:cas env-cmd .env node server.js",
    "test:realtime": "cross-env NODE_ENV=testing env-cmd .env npm run mocha realtime/sockets/realtime.test.js --require realtime/sockets/helpers.sockets.js --exit",
    "test:realtime:db": "cross-env NODE_ENV=testing env-cmd .env npm run mocha realtime/db/database.test.js --timeout=20000 --require realtime/db/helpers.db.js --exit",
    "test:realtime:t": "cross-env NODE_ENV=testing mocha app_realtime/timer/timer.test.js --require realtime/timer/helpers.js --exit",
    "test:realtime:api": "cross-env NODE_ENV=testing env-cmd .env mocha realtime/api/*.test.js --require realtime/api/helpers.js --exit",
    "cel": "browser-sync start --ws --port 5000 --no-open --no-ui",
    "mocha": "mocha",
    "bower": "bower",
    "migrate:latest": "cross-env NODE_ENV=development env-cmd .env  knex migrate:latest --knexfile  databases/relational/knexfile.js",
    "seed:run": "cross-env NODE_ENV=development env-cmd .env  knex seed:run --knexfile  databases/relational/knexfile.js",
    "development": "npm-run-all migrate:latest seed:run"
  },
  "dependencies": {
    "@keyv/mongo": "^1.0.4",
    "@xkeshi/image-compressor": "^0.5.3",
    "ajv": "^6.4.0",
    "body-parser": "~1.16.0",
    "bower": "^1.8.0",
    "cas-authentication": "^0.0.8",
    "chart.js": "^2.7.1",
    "cheerio": "^0.22.0",
    "co": "^4.6.0",
    "colors": "^1.1.2",
    "compression": "^1.7.0",
    "connect": "^3.6.0",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "debug": "~2.6.0",
    "env-cmd": "^5.1.0",
    "exceljs": "^0.4.10",
    "express": "^4.15.5",
    "express-session": "^1.15.1",
    "j-i-c": "^2.0.2",
    "jsondiffpatch": "^0.2.4",
    "keyv": "^3.0.0",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "materialnote": "^1.2.1",
    "moment": "^2.18.1",
    "moment-duration-format": "^1.3.0",
    "moment-timezone": "^0.5.13",
    "mongodb": "^2.2.25",
    "mongoose": "^5.0.9",
    "morgan": "~1.7.0",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "serve-favicon": "~2.3.2",
    "shortid": "^2.2.8",
    "simple-statistics": "^5.2.0",
    "soap": "^0.19.0",
    "socket.io": "1.7.4",
    "tracer": "^0.8.7",
    "unstream": "^2.0.0",
    "validator": "^9.4.1",
    "winston": "^3.0.0-rc0"
  },
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "chai": "^4.1.2",
    "chai-xml": "^0.3.2",
    "codecov": "^3.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^2.0.0",
    "gitbook-cli": "^2.3.2",
    "gitbook-plugin-api": "^1.1.0",
    "jsonfile": "^3.0.0",
    "lolex": "^2.3.2",
    "mocha": "^4.0.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^2.3.0",
